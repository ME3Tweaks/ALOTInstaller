{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [

        {
            "name": "LinuxDebug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildLinuxDebug",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/ALOTInstallerConsole/bin/LinuxDebug/netcoreapp3.1/ALOTInstallerConsole.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ALOTInstallerConsole",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "externalTerminal",
            "stopAtEntry": false,
            "logging": {
                "engineLogging": false,
                "moduleLoad": false,
                "exceptions": false,
                "browserStdOut": false,
                "programOutput": true,
            }
        },
        {
            "name": "LinuxRelease",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "buildLinuxRelease",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/ALOTInstallerConsole/bin/LinuxRelease/netcoreapp3.1/ALOTInstallerConsole.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ALOTInstallerConsole",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "externalTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}